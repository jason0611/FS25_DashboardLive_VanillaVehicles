			<dashboard displayType="TEXT" valueType="motorized.rpm" node="cebisAxion_dblRpmNumbersP1_SC" textColor="BOURGAULT_WHITE1" emissiveScale="0.4" textSize="0.005" textMask="0000" textAlignment="CENTER" font="GENERIC" groups="MOTOR_RUNNING PAGESC1"/>
			<dashboard displayType="TEXT" valueType="motorized.fuelUsage" node="cebisAxion_dblFuelUsageNumbersP1_SC" textColor="CLAAS_DARKGREY1" textColorDarkMode="1 1 1 1" textSize="0.004" textMask="000,0" font="GENERIC" textAlignment="LEFT" groups="MOTOR_RUNNING PAGESC1"/>
