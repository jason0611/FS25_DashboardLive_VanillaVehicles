			<dashboard displayType="TEXT" valueType="motorized.fuelUsage" node="cebisAxion_dblFuelUsageNumbersP2_SC" textColor="CLAAS_DARKGREY1" textColorDarkMode="1 1 1 1" textSize="0.004" textMask="000,0" font="GENERIC" textAlignment="LEFT" groups="MOTOR_RUNNING PAGESC2"/>
