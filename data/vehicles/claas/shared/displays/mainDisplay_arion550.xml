			<!-- Page2 -->
			<dashboard displayType="TEXT" valueType="enterable.operatingTime" node="cebisAxion_dblOperatingTimeNumbersP2_SC" textColor="CLAAS_DARKGREY1" textColorDarkMode="1 1 1 1" textSize="0.004" textMask="0000,0" font="GENERIC" textAlignment="LEFT" groups="MOTOR_RUNNING PAGESC2"/>
			
