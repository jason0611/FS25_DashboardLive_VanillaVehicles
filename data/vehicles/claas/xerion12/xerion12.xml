    <animations>
        <animation name="wiperFrontAnimation" isKeyframe="true" soundVolumeFactor="2.0">
            <part node="wiperFront"  startTime="0.0" endTime="0.5" startRot="0 0 0" endRot="4 0 -90" />
            <part node="wiperFront"  startTime="0.5" endTime="1.0" startRot="4 0 -90" endRot="1.2 0 -180" />
            <part node="wiperFront"  startTime="1.0" endTime="1.5" startRot="1.2 0 -180" endRot="6.5 0 -270" />
            <part node="wiperFront"  startTime="1.5" endTime="2.0" startRot="6.5 0 -270" endRot="0 0 -360" />

            <sound template="wiper1" startTime="0" endTime="1" direction="1"   volumeScale="1.1" pitchScale="0.7" fadeOut="0.1" linkNode="wiperFront"/>
            <sound template="wiper2" startTime="1" endTime="2" direction="1"   volumeScale="1.1" pitchScale="0.7" fadeOut="0.1" linkNode="wiperFront"/>
            <sound template="wiper1" startTime="1" endTime="2" direction="-1"  volumeScale="1.1" pitchScale="0.7" fadeOut="0.1" linkNode="wiperFront"/>
            <sound template="wiper2" startTime="0" endTime="1" direction="-1"  volumeScale="1.1" pitchScale="0.7" fadeOut="0.1" linkNode="wiperFront"/>
        </animation>
        <animation name="wiperBackAnimation" isKeyframe="true" soundVolumeFactor="2.5">
            <part node="wiperBack" startTime="0.0" endTime="0.48" startRot="0.0 0.0 0.0" endRot="0.0 -6.269 30.598" />
            <part node="wiperBack" startTime="0.48" endTime="1.0" startRot="0.0 -6.269 30.598" endRot="0.0 -10.226 90.0" />
            <part node="wiperBack" startTime="1.0" endTime="1.52" startRot="0.0 -10.226 90.0" endRot="0.0 -7.23 148.481" />
            <part node="wiperBack" startTime="1.52" endTime="2.0" startRot="0.0 -7.23 148.481" endRot="0.0 0.0 180.0" />

            <sound template="wiper1" startTime="0" endTime="1" direction="1"   volumeScale="1.8" pitchScale="0.4" fadeOut="0.3" linkNode="wiperBack"/>
            <sound template="wiper2" startTime="1" endTime="2" direction="-1"  volumeScale="1.8" pitchScale="0.4" fadeOut="0.3" linkNode="wiperBack"/>
        </animation>
        <animation name="wiperLeftAnimation" isKeyframe="true" soundVolumeFactor="1.5">
            <part node="wiperLeft"  startTime="0.0" endTime="1" startRot="0 0 0" endRot="0 2.372 -90" />

            <sound template="wiper1" startTime="0" endTime="0.8" direction="1"   volumeScale="0.6" pitchScale="1.0" fadeOut="0.3" linkNode="wiperLeft"/>
            <sound template="wiper2" startTime="0" endTime="0.8" direction="-1"  volumeScale="0.6" pitchScale="1.0" fadeOut="0.3" linkNode="wiperLeft"/>
        </animation>
        <animation name="wiperRightAnimation" isKeyframe="true" soundVolumeFactor="1.5">
            <part node="wiperRight"  startTime="0.0" endTime="1" startRot="0 0 0" endRot="0 -2.372 90" />

            <sound template="wiper1" startTime="0" endTime="0.8" direction="1"   volumeScale="0.7" pitchScale="0.8" fadeOut="0.3" linkNode="wiperRight"/>
            <sound template="wiper2" startTime="0" endTime="0.8" direction="-1"  volumeScale="0.7" pitchScale="0.8" fadeOut="0.3" linkNode="wiperRight"/>
        </animation>
    </animations>

