        <dashboard displayType="TEXT" valueType="motorized.load" node="steeringBaseScreen_numbersLoad02" textColor="0 0.3006 0.6240 1" emissiveScale="0.6" textSize="0.006" textMask="00" font="GENERIC_BOLD" groups="MOTOR_RUNNING"/>
        <dashboard displayType="TEXT" valueType="drivable.cruiseControl" node="steeringBaseScreen_numbersCruiseControl" textColor="1 1 1 1" emissiveScale="0.4" textSize="0.006" textMask="00.0" font="GENERIC" groups="MOTOR_RUNNING"/>
        <dashboard displayType="TEXT" valueType="motorized.speed" node="steeringBaseScreen_numbersSpeed" textColor="0 0.3006 0.6240 1" emissiveScale="0.6" textSize="0.009" textScaleX="0.9" textMask="00.0" font="GENERIC_BOLD" groups="MOTOR_RUNNING"/>

