        <dashboard displayType="VISIBILITY" valueType="motorized.directionForwardExclusive" node="iconForwardStanding_decal" groups="MOTOR_RUNNING" />
        <dashboard displayType="VISIBILITY" valueType="motorized.directionForwardExclusive" node="iconForward_decal" groups="MOTOR_RUNNING" />
        <dashboard displayType="VISIBILITY" valueType="motorized.directionBackward" node="iconBackwardStanding_decal" groups="MOTOR_RUNNING" />
        <dashboard displayType="VISIBILITY" valueType="motorized.directionBackward" node="iconBackward_decal" groups="MOTOR_RUNNING" />

        <dashboard displayType="MULTI_STATE" valueType="drivable.movingDirection" node="iconsStanding" groups="MOTOR_RUNNING">
            <state value="-1" visibility="false"/>
            <state value="0" visibility="true"/>
            <state value="1" visibility="false"/>
        </dashboard>
        <dashboard displayType="MULTI_STATE" valueType="drivable.movingDirection" node="iconsDriving" groups="MOTOR_RUNNING">
            <state value="-1" visibility="true"/>
            <state value="0" visibility="false"/>
            <state value="1" visibility="true"/>
        </dashboard>

        <dashboard displayType="EMITTER" node="iconForward_decal" intensity="0.3" groups="MOTOR_RUNNING"/>
        <dashboard displayType="EMITTER" node="iconForwardStanding_decal" intensity="0.3" groups="MOTOR_RUNNING"/>
        <dashboard displayType="EMITTER" node="iconBackward_decal" intensity="0.3" groups="MOTOR_RUNNING"/>
        <dashboard displayType="EMITTER" node="iconBackwardStanding_decal" intensity="0.3" groups="MOTOR_RUNNING"/>

        <configuration id="DEUTZFAHR_STARTSCREEN">
            <dashboard displayType="VISIBILITY" node="steeringBaseStartScreenDeutz_decal" idleValue="0" groups="MOTOR_STARTING"/>
            <dashboard displayType="EMITTER" node="steeringBaseStartScreenDeutz_decal" intensity="0.1" groups="MOTOR_STARTING"/>

            <objectChange node="steeringBaseStartScreenDeutz_decal" visibilityActive="true" visibilityInactive="false"/>
        </configuration>
        <configuration id="DEUTZFAHR_EDITION">
            <dashboard displayType="EMITTER" node="logoDeutzFahr_decal" hideInactive="true" intensity="0.3" groups="MOTOR_RUNNING"/>

            <objectChange node="logoDeutzFahr_decal" visibilityActive="true" visibilityInactive="false"/>
        </configuration>
        <configuration id="WARRIOR_EDITION">
            <dashboard displayType="EMITTER" node="logoWarrior_decal" hideInactive="true" intensity="0.3" groups="MOTOR_RUNNING"/>

            <objectChange node="logoWarrior_decal" visibilityActive="true" visibilityInactive="false"/>
        </configuration>
        <configuration id="SAME_EDITION">
            <dashboard displayType="VISIBILITY" node="steeringBaseStartScreenSame_decal" idleValue="0" groups="MOTOR_STARTING"/>
            <dashboard displayType="EMITTER" node="steeringBaseStartScreenSame_decal" intensity="0.1" groups="MOTOR_STARTING"/>
            <dashboard displayType="EMITTER" node="logoSame_decal" intensity="0.1" groups="MOTOR_RUNNING"/>

            <objectChange node="steeringBaseStartScreenSame_decal" visibilityActive="true" visibilityInactive="false"/>
            <objectChange node="logoSame_decal" visibilityActive="true" visibilityInactive="false"/>
        </configuration>
    </dashboardCompound>

